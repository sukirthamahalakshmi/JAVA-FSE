public class OperatorPrecedence{
    public static void main(String[] args) {
        int expressionResult = 10 + 5 * 2 - 8 / 4;
        // Explanation:
                      // 5 * 2 = 10
                     // 8 / 4 = 2
                    // 10 + 10 - 2 = 18
         System.out.println("Result of expression 10 + 5 * 2 - 8 / 4 is: " + expressionResult);
    }
}
